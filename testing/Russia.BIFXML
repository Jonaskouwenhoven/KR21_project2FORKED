<?xml version="1.0" encoding="US-ASCII"?>

<!--Author: Andreas Sauter
    Bayesian network in XMLBIF v0.3 (BayesNet Interchange Format)
 -->

<!-- DTD for the XMLBIF 0.3 format -->
<!DOCTYPE BIF [
	<!ELEMENT BIF ( NETWORK )*>
	      <!ATTLIST BIF VERSION CDATA #REQUIRED>
	<!ELEMENT NETWORK ( NAME, ( PROPERTY | VARIABLE | DEFINITION )* )>
	<!ELEMENT NAME (#PCDATA)>
	<!ELEMENT VARIABLE ( NAME, ( OUTCOME |  PROPERTY )* ) >
	      <!ATTLIST VARIABLE TYPE (nature|decision|utility) "nature">
	<!ELEMENT OUTCOME (#PCDATA)>
	<!ELEMENT DEFINITION ( FOR | GIVEN | TABLE | PROPERTY )* >
	<!ELEMENT FOR (#PCDATA)>
	<!ELEMENT GIVEN (#PCDATA)>
	<!ELEMENT TABLE (#PCDATA)>
	<!ELEMENT PROPERTY (#PCDATA)>
]>

<BIF VERSION="0.3">
<NETWORK>
    <NAME>Lecture Example2</NAME>

    <!-- Variables-->
    <VARIABLE TYPE="nature">
        <NAME>RussiaStartingANuclearWar</NAME>
        <OUTCOME>True</OUTCOME>
        <OUTCOME>False</OUTCOME>
    </VARIABLE>
    <VARIABLE TYPE="nature">
        <NAME>UkraineWarEvents</NAME>
        <OUTCOME>True</OUTCOME>
        <OUTCOME>False</OUTCOME>
    </VARIABLE>
    <VARIABLE TYPE="nature">
        <NAME>RussiaResponseToWar</NAME>
        <OUTCOME>True</OUTCOME>
        <OUTCOME>False</OUTCOME>
    </VARIABLE>
    <VARIABLE TYPE="nature">
        <NAME>MilitaryActionByRussia</NAME>
        <OUTCOME>True</OUTCOME>
        <OUTCOME>False</OUTCOME>
    </VARIABLE>
    <VARIABLE TYPE="nature">
        <NAME>MilitaryActionByUkraine</NAME>
        <OUTCOME>True</OUTCOME>
        <OUTCOME>False</OUTCOME>
    </VARIABLE>
    <VARIABLE TYPE="nature">
        <NAME>EconomicSanctionsAgainstRussia</NAME>
        <OUTCOME>True</OUTCOME>
        <OUTCOME>False</OUTCOME>
    </VARIABLE>
    <VARIABLE TYPE="nature">
        <NAME>PoliticalDevelopmentsInUkraine</NAME>
        <OUTCOME>True</OUTCOME>
        <OUTCOME>False</OUTCOME>
    </VARIABLE>
    <VARIABLE TYPE="nature">
        <NAME>DomesticSupportForRussiaActions</NAME>
        <OUTCOME>True</OUTCOME>
        <OUTCOME>False</OUTCOME>
    </VARIABLE>
    <VARIABLE TYPE="nature">
        <NAME>InternationalSupportForUkraine</NAME>
        <OUTCOME>True</OUTCOME>
        <OUTCOME>False</OUTCOME>
    </VARIABLE>
    <VARIABLE TYPE="nature">
        <NAME>RussianEconomy</NAME>
        <OUTCOME>True</OUTCOME>
        <OUTCOME>False</OUTCOME>
    </VARIABLE>
    <!-- PCTs -->
    <DEFINITION> <!--UkraineWarEvents-->
        <FOR>UkraineWarEvents</FOR>
        <TABLE>0.5 0.5</TABLE>
    </DEFINITION>
    <DEFINITION> <!--PoliticalDevelopmentsInUkraine-->
        <FOR>PoliticalDevelopmentsInUkraine</FOR>
        <TABLE>0.5 0.5</TABLE>
    </DEFINITION>
    <DEFINITION> <!--RussiaResponseToWar-->
        <FOR>RussiaResponseToWar</FOR>
        <TABLE>0.5 0.5</TABLE>
    </DEFINITION>
    <DEFINITION>
        <FOR>MilitaryActionByUkraine</FOR>
        <GIVEN>UkraineWarEvents</GIVEN>
        <GIVEN>PoliticalDevelopmentsInUkraine</GIVEN>
        <TABLE>0.1 0.9 0.8 0.2 0.8 0.2 0.1 0.9</TABLE>
    </DEFINITION>
    <DEFINITION>
        <FOR>MilitaryActionByRussia</FOR>
        <GIVEN>UkraineWarEvents</GIVEN>
        <GIVEN>RussiaResponseToWar</GIVEN>
        <TABLE>0.2 0.8 0.4 0.6 0.3 0.7 0.6 0.4</TABLE>
    </DEFINITION>
    <DEFINITION>
        <FOR>DomesticSupportForRussiaActions</FOR>
        <GIVEN>UkraineWarEvents</GIVEN>
        <GIVEN>RussiaResponseToWar</GIVEN>
        <GIVEN>MilitaryActionByUkraine</GIVEN>
         <TABLE>0.1 0.9 0.2 0.8 0.6 0.4 0.4 0.6 0.3 0.7 0.8 0.2 0.7 0.3 0.9 0.1</TABLE>
    </DEFINITION>
    <DEFINITION>
        <FOR>InternationalSupportForUkraine</FOR>
        <GIVEN>UkraineWarEvents</GIVEN>
        <GIVEN>RussiaResponseToWar</GIVEN>
        <!-- <TABLE>TODO give Values</TABLE> -->
        <TABLE>0.1 0.9 0.2 0.8 0.6 0.4 0.7 0.3</TABLE>
    </DEFINITION>
    <DEFINITION>
        <FOR>EconomicSanctionsAgainstRussia</FOR>
        <GIVEN>UkraineWarEvents</GIVEN>
        <GIVEN>InternationalSupportForUkraine</GIVEN>
        <TABLE>0.1 0.9 0.5 0.5 0.8 0.2 0.6 0.4</TABLE>
    </DEFINITION>
    <DEFINITION>
        <FOR>RussianEconomy</FOR>
        <GIVEN>MilitaryActionByRussia</GIVEN>
        <GIVEN>EconomicSanctionsAgainstRussia</GIVEN>
    <TABLE>0.1 0.9 0.5 0.5 0.8 0.2 0.6 0.4</TABLE>
    </DEFINITION>
    <DEFINITION>
        <FOR>RussiaStartingANuclearWar</FOR>
        <GIVEN>MilitaryActionByUkraine</GIVEN>
        <GIVEN>DomesticSupportForRussiaActions</GIVEN>
        <GIVEN>RussianEconomy</GIVEN>
         <TABLE>0.1 0.9 0.2 0.8 0.6 0.4 0.4 0.6 0.3 0.7 0.8 0.2 0.7 0.3 0.9 0.1</TABLE>
    </DEFINITION>
</NETWORK>
</BIF>